{
  "name": "Prongs",
  "version": "1.0.0",
  "description": "Personal Site Built with Eleventy",
  "repository": {
    "type": "git",
    "url": "https://github.com/anyuzx/Prongs.git"
  },
  "main": "index.html",
  "scripts": {
    "clean": "rm -rf dist",
    "11ty:debug": "DEBUG=Eleventy* npx @11ty/eleventy --dryrun",
    "serve": "npx @11ty/eleventy --serve",
    "build:11ty": "npx @11ty/eleventy",
    "css:preprocess": "postcss src/_includes/css/main.css -o dist/_includes/css/main.css --verbose",
    "cms:bundle": "rollup --config",
    "build": "ELEVENTY_ENV=production run-s clean cms:bundle css:preprocess build:11ty",
    "build:refresh-data": "FETCH_REMOTE_DATA=1 ELEVENTY_ENV=production run-s clean cms:bundle css:preprocess build:11ty",
    "build:development": "ELEVENTY_ENV=development run-s clean css:preprocess build:11ty",
    "build:serve": "ELEVENTY_ENV=development run-s clean css:preprocess serve"
  },
  "keywords": [],
  "author": "Guang Shi",
  "engines": {
    "node": ">=18"
  },
  "license": "ISC",
  "devDependencies": {
    "@11ty/eleventy": "^3.1.2",
    "@fullhuman/postcss-purgecss": "^2.2.0",
    "@rollup/plugin-commonjs": "^29.0.0",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^16.0.3",
    "autoprefixer": "^10.4.24",
    "bl": "^4.0.3",
    "critical": "^2.0.0-27",
    "cssnano": "^7.1.2",
    "dotenv": "^16.4.7",
    "eslint": "^7.2.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "minimist": "^1.2.5",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.5.6",
    "postcss-cli": "^11.0.1",
    "postcss-import": "^16.1.1",
    "posthtml": "^0.13.0",
    "posthtml-inline-assets": "^3.1.0",
    "rollup": "^4.57.1",
    "rollup-plugin-polyfill-node": "^0.13.0",
    "tailwindcss": "^3.4.19",
    "watch": "^1.0.2"
  },
  "dependencies": {
    "@11ty/eleventy-plugin-rss": "^2.0.4",
    "@babel/core": "^7.10.2",
    "@iktakahiro/markdown-it-katex": "^3.1.0",
    "axios": "^1.13.5",
    "babel-plugin-prismjs": "^2.0.1",
    "citation-js": "^0.5.0-alpha.5",
    "cloudinary": "^1.22.0",
    "dayjs": "^1.8.28",
    "highlight.js": "^10.4.1",
    "htm": "^3.0.4",
    "html-minifier": "^4.0.0",
    "js-yaml": "^4.1.1",
    "make-dir": "^3.1.0",
    "make-dir-cli": "^2.0.0",
    "markdown-it": "^12.3.2",
    "markdown-it-abbr": "^1.0.4",
    "markdown-it-anchor": "^5.3.0",
    "markdown-it-attrs": "^4.3.1",
    "markdown-it-container": "^2.0.0",
    "markdown-it-emoji": "^1.4.0",
    "markdown-it-footnote": "^3.0.2",
    "markdown-it-implicit-figures": "^0.10.0",
    "markdown-it-ins": "^3.0.0",
    "markdown-it-kbd": "^2.0.0",
    "markdown-it-mark": "^3.0.0",
    "markdown-it-sub": "^1.0.0",
    "markdown-it-sup": "^1.0.0",
    "markdown-it-toc-done-right": "^4.1.0",
    "netlify-plugin-a11y": "0.0.11",
    "postcss-csso": "^6.0.0",
    "shelljs": "^0.8.5",
    "terser": "^4.7.0",
    "typeset": "^0.3.4",
    "typography": "^0.16.19",
    "typography-theme-bootstrap": "^0.16.19",
    "workbox-build": "^7.4.0"
  },
  "resolutions": {
    "minimist": "^1.2.5"
  },
  "preinstall": "npx npm-force-resolutions"
}
