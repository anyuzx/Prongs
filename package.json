{
  "name": "Prongs",
  "version": "0.7.0",
  "description": "Personal site",
  "repository": {
    "type": "git",
    "url": "https://github.com/anyuzx/Prongs.git"
  },
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean": "rm -rf dist",
    "11ty:debug": "DEBUG=Eleventy* npx @11ty/eleventy --dryrun",
    "build:css": "postcss dist/_includes/css/*.css --dir dist/_includes/css/",
    "build:11ty": "npx @11ty/eleventy",
    "build": "ELEVENTY_ENV=production run-s clean cms:bundle build:11ty build:css css:critical",
    "build:development": "ELEVENTY_ENV=development run-s clean build:11ty build:css",
    "start:css": "postcss dist/_includes/css/*.css --dir dist/_includes/css/",
    "start:11ty": "npx @11ty/eleventy --quiet --serve",
    "prestart": "run-s clean build:11ty",
    "watch:css": "watch 'npm run start:css' ./src",
    "start": "run-p start:11ty watch:css",
    "css:critical": "node criticalCSS.js",
    "cms:precompile": "make-dir dist/admin && nunjucks-precompile src/_includes/layouts/post.njk > dist/admin/templates.js -i '\\.(njk|css|svg)$'",
    "cms:bundle": "rollup --config"
  },
  "keywords": [],
  "author": "Guang Shi",
  "license": "ISC",
  "devDependencies": {
    "@11ty/eleventy": "^0.11.0",
    "@fullhuman/postcss-purgecss": "^2.2.0",
    "bl": "^4.0.2",
    "critical": "^2.0.0-21",
    "eslint": "^7.0.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "minimist": "^1.2.5",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^7.1.1",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-resolve": "^5.2.0",
    "tailwindcss-dark-mode": "^1.1.4",
    "watch": "^1.0.2"
  },
  "dependencies": {
    "@11ty/eleventy-plugin-inclusive-language": "^1.0.0",
    "@11ty/eleventy-plugin-rss": "^1.0.7",
    "@11ty/eleventy-plugin-syntaxhighlight": "^3.0.1",
    "@babel/core": "^7.9.6",
    "@iktakahiro/markdown-it-katex": "^3.1.0",
    "babel-plugin-prismjs": "^2.0.1",
    "cloudinary": "^1.21.0",
    "cssnano": "^4.1.10",
    "dayjs": "^1.8.27",
    "highlight.js": "^10.0.3",
    "html-minifier": "^4.0.0",
    "js-yaml": "^3.13.1",
    "make-dir": "^3.1.0",
    "make-dir-cli": "^2.0.0",
    "markdown-it": "^10.0.0",
    "markdown-it-abbr": "^1.0.4",
    "markdown-it-anchor": "^5.3.0",
    "markdown-it-attrs": "^3.0.2",
    "markdown-it-container": "^2.0.0",
    "markdown-it-emoji": "^1.4.0",
    "markdown-it-footnote": "^3.0.2",
    "markdown-it-implicit-figures": "^0.10.0",
    "markdown-it-ins": "^3.0.0",
    "markdown-it-kbd": "^2.0.0",
    "markdown-it-mark": "^3.0.0",
    "markdown-it-sub": "^1.0.0",
    "markdown-it-sup": "^1.0.0",
    "markdown-it-toc-done-right": "^4.1.0",
    "rollup": "^2.10.2",
    "rollup-plugin-babel": "^4.4.0",
    "tailwindcss": "^1.4.6",
    "tailwindcss-typography": "^3.1.0",
    "typeset": "^0.2.9",
    "typography": "^0.16.19",
    "typography-theme-bootstrap": "^0.16.19"
  },
  "resolutions": {
    "minimist": "^1.2.5"
  },
  "preinstall": "npx npm-force-resolutions"
}
